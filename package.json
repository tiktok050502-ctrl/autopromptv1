{
  "name": "autoprompt",
  "version": "1.0.0",
  "private": true,
  "main": "main.js",
  "description": "AutoPrompt - AI Video & Script Generator",
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build:web": "tsc && vite build",
    "start": "cross-env NODE_ENV=development electron .",
    "make": "electron-builder"
  },
  "dependencies": {
    "@google/genai": "^0.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.26",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "build": {
    "appId": "com.autoprompt.app",
    "productName": "AutoPrompt",
    "files": [
      "dist/**/*",
      "main.js",
      "icon.ico"
    ],
    "asar": false,
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
